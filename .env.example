# Generate a random secret: https://generate-secret.vercel.app/32 or `openssl rand -base64 32`
AUTH_SECRET=your_auth_secret_here

# Get your xAI API Key here for chat and image models: https://console.x.ai/
XAI_API_KEY=your_xai_api_key_here

# Instructions to create a Vercel Blob Store here: https://vercel.com/docs/storage/vercel-blob
BLOB_READ_WRITE_TOKEN=your_blob_token_here

# Your Supabase/PostgreSQL connection URL
POSTGRES_URL=postgres://user:password@host:5432/database

# Supabase Auth Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_URL=your_supabase_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Supabase URLs
DATABASE_URL=postgresql://user:password@host:6543/postgres?pgbouncer=true&ssl=true&sslmode=require&connection_limit=1
DATABASE_TRANSACTION_POOLER_URL=postgresql://user:password@host:5432/postgres?sslmode=require
DIRECT_URL=postgresql://user:password@host:5432/postgres?ssl=true&sslmode=require&connection_limit=1
POSTGRES_URL=postgresql://user:password@host:5432/postgres?sslmode=require&pool_timeout=0

# Other Keys
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GOOGLE_API_KEY=your_google_api_key_here
GOOGLE_GENERATIVE_AI_API_KEY=your_google_ai_api_key_here 